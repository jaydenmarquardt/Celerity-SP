{
  "name": "@celerity/sp",
  "version": "0.0.1",
  "private": false,
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18.20.3"
  },
  "main": "lib/index.js",
  "scripts": {
    "init": "npm install; npm run deploy; npm link;",
    "deploy": "npm run clean; gulp bundle --max_old_space_size=16384 --ship --verbose;",
    "pack": "npm run deploy; gulp package-solution --max_old_space_size=16384 --ship;",
    "ship": "npm run update; npm run deploy; gulp package-solution --ship; npm run push;",
    "update": "node ./scripts/update/update.ts;",
    "push": "node ./scripts/push/push.ts;",
    "build": "gulp bundle",
    "clean": "gulp clean",
    "test": "gulp test",
    "serve": "gulp bundle --custom-serve --max_old_space_size=16384 && fast-serve",
    "lint": "npm run lint:js; npm run lint:prettier; npm run lint:scss",
    "lint:fix": "npm run lint:js-fix; npm run lint:scss-fix; npm run lint:prettier-fix",
    "lint:js": "eslint --ext .ts,.tsx 'src/**/*.{ts,tsx}' 'src/components/**/**/*.{ts,tsx}' --ignore-pattern '*.scss.ts'",
    "lint:js-fix": "eslint --fix 'src/**/*.{ts,tsx}' 'src/components/**/**/*.{ts,tsx}' --ignore-pattern '*.scss.ts'",
    "lint:prettier": "npx prettier --check src --ignore-unknown",
    "lint:prettier-fix": "npx prettier --write src --ignore-unknown",
    "lint:scss": "npx stylelint src/components/*/*/*.scss",
    "lint:scss-fix": "npx stylelint --fix src/components/*/*/*.scss"
  },
  "dependencies": {
    "@microsoft/sp-application-base": "^1.19.0",
    "@microsoft/sp-core-library": "^1.19.0",
    "@microsoft/sp-dialog": "^1.19.0",
    "@microsoft/sp-http": "^1.19.0",
    "@microsoft/sp-image-helper": "^1.19.0",
    "@microsoft/sp-listview-extensibility": "^1.19.0",
    "@microsoft/sp-lodash-subset": "^1.19.0",
    "@microsoft/sp-office-ui-fabric-core": "^1.19.0",
    "@microsoft/sp-property-pane": "^1.19.0",
    "@microsoft/sp-webpart-base": "^1.19.0",
    "@pnp/core": "^4.3.0",
    "@pnp/graph": "^4.3.0",
    "@pnp/logging": "^4.3.0",
    "@pnp/sp": "^4.3.0",
    "@pnp/spfx-controls-react": "^3.12.0",
    "@pnp/spfx-property-controls": "3.13",
    "es6-promise": "^4.2.8",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-table": "^7.7.0"
  },
  "devDependencies": {
    "@microsoft/eslint-config-spfx": "1.20.1",
    "@microsoft/eslint-plugin-spfx": "1.20.1",
    "@microsoft/rush-stack-compiler-4.5": "0.5.1",
    "@microsoft/sp-build-web": "1.20.1",
    "@microsoft/sp-module-interfaces": "1.20.1",
    "@microsoft/sp-tslint-rules": "1.20.1",
    "@rushstack/eslint-config": "2.5.1",
    "@types/react": "17.0.45",
    "@types/react-dom": "17.0.17",
    "@types/react-table": "^7.7.0",
    "@types/webpack-env": "~1.15.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.35.1",
    "ajv": "^6.12.5",
    "es6-promise": "^4.2.8",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-etc": "^2.0.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-security": "^1.7.1",
    "gulp": "4.0.2",
    "prettier": "^2.7.1",
    "spfx-fast-serve": "^3.0.6",
    "spfx-fast-serve-helpers": "^1.19.0",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.3.0",
    "tslib": "2.3.1",
    "twig": "^1.16.0",
    "typescript": "^4.5.5",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.9.0"
  }
}
